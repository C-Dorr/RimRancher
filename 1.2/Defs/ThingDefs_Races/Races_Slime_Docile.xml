<!--===================== Puddle Slime ===================-->
<!--======================================================-->

<ThingDef ParentName="SlimeThingBase">
		<defName>DOOR_Puddle_Slime</defName>
		<label>puddle slime</label>
		<!--Description taken from https://slimerancher.fandom.com/wiki/Pink_Slime -->
		<description>Puddle slimes are a rare species of slime that live in fresh water pools found on the range. These slimes don't eat fruits, veggies, or meat like most other slimes. Instead, puddle slimes absorb water into their bodies while they sit on the surface of a pool.Puddle slimes need fresh water to survive and will quickly evaporate if left on any other surface for too long. (RimRancher has taken liberties with this as of now; puddle slimes do not need water to survive and are omnivorous.)</description>
		<statBases>
			<MoveSpeed>4.0</MoveSpeed>
			<ComfyTemperatureMin>0</ComfyTemperatureMin>
		</statBases>
		<comps>
			<li Class="RimRancher.CompProperties_PlortLayer">
				<plortCountRange>1</plortCountRange>
				<plortDef>DOOR_PuddlePlort</plortDef>
				<plortLayIntervalDays>0.25</plortLayIntervalDays>
			</li>
		</comps>
		<race>
			<body>DOOR_Slime_General</body>
			<petness>1.0</petness>
			<predator>false</predator>
			<foodType>OmnivoreAnimal</foodType>
		</race>
	</ThingDef>

	<PawnKindDef ParentName="AnimalKindBase">
		<defName>DOOR_Puddle_Slime</defName>
		<label>Puddle Slime</label>
		<race>DOOR_Puddle_Slime</race>
		<ecoSystemWeight>0.1</ecoSystemWeight>
		<combatPower>0.0</combatPower>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Resource/Slime/Puddle_Slime/Puddle_Slime</texPath>
					<drawSize>0.7</drawSize>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Resource/Slime/Dessicated_Slime</texPath>
					<drawSize>1.0</drawSize>
				</dessicatedBodyGraphicData>
			</li>
		</lifeStages>
	</PawnKindDef>

