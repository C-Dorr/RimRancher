	<!--==================== Tabby Slime =====================-->
	<!--======================================================-->
	
	<ThingDef ParentName="SlimeThingBase">
		<defName>DOOR_Tabby_Slime</defName>
		<label>tabby slime</label>
		<!--Description taken from https://slimerancher.fandom.com/wiki/Tabby_Slime -->
		<description>Tabby slimes share many curious similarities to domesticated house cats of Earth. With their perky ears, striped backs, and swaying tails, you'd almost mistake them for a true feline... if they weren't made entirely of gooey slime, of course.</description>
		<statBases>
			<MoveSpeed>4.0</MoveSpeed>
			<ComfyTemperatureMin>0</ComfyTemperatureMin>
		</statBases>
		<comps>
			<li Class="RimRancher.CompProperties_PlortLayer">
				<plortCountRange>1</plortCountRange>
				<plortDef>DOOR_TabbyPlort</plortDef>
				<plortLayIntervalDays>0.25</plortLayIntervalDays>
			</li>
		</comps>
		<race>
			<body>DOOR_Slime_Cat</body>
			<wildness>0</wildness>
			<petness>1.0</petness>
			<predator>true</predator>
			<manhunterOnDamageChance>0.10</manhunterOnDamageChance>
			<foodType>CarnivoreAnimal</foodType>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1)</li>
					<li>(2.5, 0)</li>
				</points>
			</litterSizeCurve>
		</race>
	</ThingDef>

	<PawnKindDef ParentName="AnimalKindBase">
